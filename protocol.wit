package example:protocol;

interface types {
  record point {
    x: s32,
    y: s32,
  }
}

world my-world {
  use types.{point};

  // Say "Hello" to stdout, use with wasi
  export say-hello: func();

  // Read line from stdin, use with wasi
  export read-line: func() -> option<string>;

  export add-three: func(num: u64) -> u64;
}
